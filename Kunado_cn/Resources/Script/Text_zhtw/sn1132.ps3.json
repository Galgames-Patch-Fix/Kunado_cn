		},
		{
			"Text_Raw": "…―─",
			"Text_Tra": "…―─",
			"FOA_Ptr": "0x0001AC98",
			"FOA_Rva": "0x000400A3"
		},
		{
			"Text_Raw": "。、」』）！？”～ー♪",
			"Text_Tra": "。、」』）！？”～ー♪",
			"FOA_Ptr": "0x0001ACA4",
			"FOA_Rva": "0x000400AA"
		},
		{
			"Text_Raw": "- 優里本筋０８－０２ -",
			"Text_Tra": "優里主線08-02",
			"FOA_Ptr": "0x0003D7DC",
			"FOA_Rva": "0x00040563"
		},
		{
			"Text_Raw": "「もっと先の未来、か」",
			"Text_Tra": "「更加遙遠的未來啊。」",
			"FOA_Ptr": "0x0003D80A",
			"FOA_Rva": "0x0004057A"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D822",
			"FOA_Rva": "0x00040592"
		},
		{
			"Text_Raw": "優里の言葉を、そのまま素直に心に受け入れる。",
			"Text_Tra": "我就這麼將優里所說的放進了心底。",
			"FOA_Ptr": "0x0003D862",
			"FOA_Rva": "0x00040599"
		},
		{
			"Text_Raw": "それは未来で。",
			"Text_Tra": "那是未來。",
			"FOA_Ptr": "0x0003D8EE",
			"FOA_Rva": "0x000405C8"
		},
		{
			"Text_Raw": "だから、希望だ。",
			"Text_Tra": "所以充滿希望。",
			"FOA_Ptr": "0x0003D946",
			"FOA_Rva": "0x000405D9"
		},
		{
			"Text_Raw": "「いいな」",
			"Text_Tra": "「真好啊。」",
			"FOA_Ptr": "0x0003D99E",
			"FOA_Rva": "0x000405EC"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D9B6",
			"FOA_Rva": "0x000405F8"
		},
		{
			"Text_Raw": "それは……いいなぁ。",
			"Text_Tra": "那可……真是好啊。",
			"FOA_Ptr": "0x0003D9F6",
			"FOA_Rva": "0x000405FF"
		},
		{
			"Text_Raw": "もちろん、希望などと呼ぶものに明確な形はなく、求めたことで逆に後悔することもあるだろう。",
			"Text_Tra": "當然，這叫做希望的東西是沒有明確形體的，也有可能會讓人後悔去追求它。",
			"FOA_Ptr": "0x0003DA4E",
			"FOA_Rva": "0x00040616"
		},
		{
			"Text_Raw": "しかし、それは目的地を定めない旅につきまとう期待と不安のようなものだ。",
			"Text_Tra": "但是，那就像是沒有目的地的旅行所伴隨的期待與不安那樣的東西。",
			"FOA_Ptr": "0x0003DAA6",
			"FOA_Rva": "0x00040671"
		},
		{
			"Text_Raw": "１枚のコインの裏表のようなもの。",
			"Text_Tra": "如同一枚硬幣的表裡",
			"FOA_Ptr": "0x0003DAFE",
			"FOA_Rva": "0x000406BA"
		},
		{
			"Text_Raw": "足が前に進むのであれば、希望も後悔も、どちらも楽しめばいい。",
			"Text_Tra": "邁開腳步的話，不管是希望還是後悔，都只要享受它就好了。",
			"FOA_Ptr": "0x0003DB56",
			"FOA_Rva": "0x000406DD"
		},
		{
			"Text_Raw": "「うん。僕は見せるよ」",
			"Text_Tra": "「嗯。我會給你們看的。」",
			"FOA_Ptr": "0x0003DC82",
			"FOA_Rva": "0x0004071C"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DC9A",
			"FOA_Rva": "0x00040734"
		},
		{
			"Text_Raw": "声が漏れ出していた。",
			"Text_Tra": "不禁說出了聲。",
			"FOA_Ptr": "0x0003DCDA",
			"FOA_Rva": "0x0004073B"
		},
		{
			"Text_Raw": "自分が話し出したことを理解し、心のままに続ける。",
			"Text_Tra": "我明白自己所說的，並且就這麼繼續說道。",
			"FOA_Ptr": "0x0003DD32",
			"FOA_Rva": "0x00040752"
		},
		{
			"Text_Raw": "「優里だけでなく、カントのみんなにも、僕自身も含めて――」",
			"Text_Tra": "「不只是優里，還有康托的大家，也包括我自己――」",
			"FOA_Ptr": "0x0003DD8A",
			"FOA_Rva": "0x00040785"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DDA2",
			"FOA_Rva": "0x000407C1"
		},
		{
			"Text_Raw": "「“もっと先にある未来を見せる”」",
			"Text_Tra": "「『給你們看看更遙遠的未來。』」",
			"FOA_Ptr": "0x0003DE26",
			"FOA_Rva": "0x000407C8"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DE3E",
			"FOA_Rva": "0x000407EC"
		},
		{
			"Text_Raw": "断定が言霊になる。",
			"Text_Tra": "認定化做了言靈。",
			"FOA_Ptr": "0x0003DEAA",
			"FOA_Rva": "0x000407F3"
		},
		{
			"Text_Raw": "「はい。見せてください」",
			"Text_Tra": "「是。請讓我看看吧。」",
			"FOA_Ptr": "0x0003DF02",
			"FOA_Rva": "0x00040808"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003DF1A",
			"FOA_Rva": "0x00040831"
		},
		{
			"Text_Raw": "優里も笑顔のまま、小さく、それでいて力強くうなずいてくれる。",
			"Text_Tra": "優里也帶著笑容，小而堅定的點了點頭。",
			"FOA_Ptr": "0x0003DF5A",
			"FOA_Rva": "0x00040838"
		},
		{
			"Text_Raw": "こうして僕はさらに一歩を踏み出した。",
			"Text_Tra": "於是我又踏出了一步。",
			"FOA_Ptr": "0x0003DFB2",
			"FOA_Rva": "0x00040877"
		},
		{
			"Text_Raw": "選んだ。",
			"Text_Tra": "選擇。",
			"FOA_Ptr": "0x0003E00A",
			"FOA_Rva": "0x0004089E"
		},
		{
			"Text_Raw": "選んでしまった。",
			"Text_Tra": "我做了選擇。",
			"FOA_Ptr": "0x0003E062",
			"FOA_Rva": "0x000408A9"
		},
		{
			"Text_Raw": "「うーん……それでも、もったいない……すごくもったいない」",
			"Text_Tra": "「嗯……但是這樣好可惜……真的好可惜哦。」",
			"FOA_Ptr": "0x0003E0BA",
			"FOA_Rva": "0x000408BC"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E0D2",
			"FOA_Rva": "0x000408F8"
		},
		{
			"Text_Raw": "わかっていても、がっくりと肩が落ちてしまう。",
			"Text_Tra": "雖然了然於心，但還是難免失望。",
			"FOA_Ptr": "0x0003E112",
			"FOA_Rva": "0x000408FF"
		},
		{
			"Text_Raw": "「なんです？　もったいない？」",
			"Text_Tra": "「可惜？　可惜什麼啊？」",
			"FOA_Ptr": "0x0003E16A",
			"FOA_Rva": "0x0004092E"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E182",
			"FOA_Rva": "0x0004095D"
		},
		{
			"Text_Raw": "「いや、優里を選ばなかったことが、ね」",
			"Text_Tra": "「可惜沒有選擇優里啊。」",
			"FOA_Ptr": "0x0003E1C2",
			"FOA_Rva": "0x00040964"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E1DA",
			"FOA_Rva": "0x0004098C"
		},
		{
			"Text_Raw": "当然だ。",
			"Text_Tra": "當然啦。",
			"FOA_Ptr": "0x0003E21A",
			"FOA_Rva": "0x00040993"
		},
		{
			"Text_Raw": "これほど可愛い女の子に、僕がなにか新しいことをする以上に、自分が独占しますと言われていたのだ。",
			"Text_Tra": "被這麼可愛的女孩子說，之後不管我做什麼新的事情，她都要獨占我。",
			"FOA_Ptr": "0x0003E272",
			"FOA_Rva": "0x0004099E"
		},
		{
			"Text_Raw": "独占されたかった。",
			"Text_Tra": "當然會想被她獨佔啊。",
			"FOA_Ptr": "0x0003E2CA",
			"FOA_Rva": "0x000409FF"
		},
		{
			"Text_Raw": "滅茶苦茶にされたかった。",
			"Text_Tra": "超級想的。",
			"FOA_Ptr": "0x0003E322",
			"FOA_Rva": "0x00040A14"
		},
		{
			"Text_Raw": "だって、ほら、優里のような固い子は、お布団のうえではものすごく乱れそうだし――",
			"Text_Tra": "因為你看，像優里這種頑固的女孩，在床上感覺一定很――",
			"FOA_Ptr": "0x0003E37A",
			"FOA_Rva": "0x00040A2F"
		},
		{
			"Text_Raw": "「……あの、なにか不穏なことを考えていませんか？」",
			"Text_Tra": "「……那個，你是不是在想什麼糟糕的事情？」",
			"FOA_Ptr": "0x0003E3D2",
			"FOA_Rva": "0x00040A80"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E3EA",
			"FOA_Rva": "0x00040AC3"
		},
		{
			"Text_Raw": "「そ、そんなことないあるよ」",
			"Text_Tra": "「我、我沒有不這樣想哦。」",
			"FOA_Ptr": "0x0003E42A",
			"FOA_Rva": "0x00040ACA"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E442",
			"FOA_Rva": "0x00040AE8"
		},
		{
			"Text_Raw": "「どっちですか！？」",
			"Text_Tra": "「到底有沒有！？」",
			"FOA_Ptr": "0x0003E482",
			"FOA_Rva": "0x00040AEF"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E49A",
			"FOA_Rva": "0x00040B14"
		},
		{
			"Text_Raw": "「いや、嘘がつけなくて」",
			"Text_Tra": "「沒有啦，我真的沒有騙你。」",
			"FOA_Ptr": "0x0003E4DA",
			"FOA_Rva": "0x00040B1B"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E4F2",
			"FOA_Rva": "0x00040B35"
		},
		{
			"Text_Raw": "思わず苦笑いしてしまう。",
			"Text_Tra": "我苦笑了起來。",
			"FOA_Ptr": "0x0003E532",
			"FOA_Rva": "0x00040B3C"
		},
		{
			"Text_Raw": "「まったく。こんなときまで……仕方のない人ですね」",
			"Text_Tra": "「真是的。連這種時候都……真是拿你沒辦法耶。」",
			"FOA_Ptr": "0x0003E58A",
			"FOA_Rva": "0x00040B57"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E5A2",
			"FOA_Rva": "0x00040B9A"
		},
		{
			"Text_Raw": "「でも、その……私を選ばなかったことをもったいないと肩を落とすほど残念に思ってくれていることは、すごく嬉しい、です」",
			"Text_Tra": "「可是啊，嗯……我很高興你覺得沒有選我非常可惜，而且還那麼沮喪。」",
			"FOA_Ptr": "0x0003E5E2",
			"FOA_Rva": "0x00040BA1"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E5FA",
			"FOA_Rva": "0x00040C26"
		},
		{
			"Text_Raw": "嘘をつけない優里も、照れ臭そうというより、くすぐったそうに笑う。",
			"Text_Tra": "不會說謊的優里也笑了，那笑容與其說是害羞，不如說是像被搔癢。",
			"FOA_Ptr": "0x0003E63A",
			"FOA_Rva": "0x00040C2D"
		},
		{
			"Text_Raw": "ありがとう。",
			"Text_Tra": "謝謝。",
			"FOA_Ptr": "0x0003E692",
			"FOA_Rva": "0x00040C70"
		},
		{
			"Text_Raw": "「ありがとう、優里」",
			"Text_Tra": "「謝謝妳，優里。」",
			"FOA_Ptr": "0x0003E6EA",
			"FOA_Rva": "0x00040C7F"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E702",
			"FOA_Rva": "0x00040C95"
		},
		{
			"Text_Raw": "「ありがとう、信様」",
			"Text_Tra": "「謝謝你，信大人。」",
			"FOA_Ptr": "0x0003E742",
			"FOA_Rva": "0x00040C9C"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E75A",
			"FOA_Rva": "0x00040CC1"
		},
		{
			"Text_Raw": "互いに感謝を告げる。",
			"Text_Tra": "我們彼此互道感謝。",
			"FOA_Ptr": "0x0003E79A",
			"FOA_Rva": "0x00040CC8"
		},
		{
			"Text_Raw": "「それなら、私も新しい目標に向かって進みます」",
			"Text_Tra": "「這樣的話，我也要朝新的目標前進了。」",
			"FOA_Ptr": "0x0003E7F2",
			"FOA_Rva": "0x00040CDF"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E80A",
			"FOA_Rva": "0x00040D1E"
		},
		{
			"Text_Raw": "「新しい目標？」",
			"Text_Tra": "「新的目標？」",
			"FOA_Ptr": "0x0003E84A",
			"FOA_Rva": "0x00040D25"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E862",
			"FOA_Rva": "0x00040D37"
		},
		{
			"Text_Raw": "「春姫様から話を頂いたのですが、私を中心に、八剣とは別に護衛隊というものを組織したいんだそうです」",
			"Text_Tra": "「春姬大人跟我説了，好像是要以我為中心組織一個有別於八劍，名叫護衛隊的組織。」",
			"FOA_Ptr": "0x0003E8A2",
			"FOA_Rva": "0x00040D3E"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003E8BA",
			"FOA_Rva": "0x00040DB1"
		},
		{
			"Text_Raw": "「ああ、あれか」",
			"Text_Tra": "「哦哦，那個啊。」",
			"FOA_Ptr": "0x0003E8FA",
			"FOA_Rva": "0x00040DB8"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E912",
			"FOA_Rva": "0x00040DCA"
		},
		{
			"Text_Raw": "警察や軍隊に繋がる機構。",
			"Text_Tra": "跟警察還有軍隊相關聯的機構。",
			"FOA_Ptr": "0x0003E952",
			"FOA_Rva": "0x00040DD1"
		},
		{
			"Text_Raw": "「あの話、優里は請けるつもりなんだ」",
			"Text_Tra": "「所以優里也有意要答應就是了。」",
			"FOA_Ptr": "0x0003E9AA",
			"FOA_Rva": "0x00040DEC"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E9C2",
			"FOA_Rva": "0x00040E12"
		},
		{
			"Text_Raw": "「そうしようと思います」",
			"Text_Tra": "「我是這麼想的。」",
			"FOA_Ptr": "0x0003EA02",
			"FOA_Rva": "0x00040E19"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003EA1A",
			"FOA_Rva": "0x00040E42"
		},
		{
			"Text_Raw": "優里がうなずく。",
			"Text_Tra": "優里點了點頭。",
			"FOA_Ptr": "0x0003EA5A",
			"FOA_Rva": "0x00040E49"
		},
		{
			"Text_Raw": "「あなたや春姫様が剣であり前を向いているとき、護衛隊は盾として背中をお守りいたします」",
			"Text_Tra": "「當你或春姬大人向前為劍時，護衛隊就化為守住背後的盾。」",
			"FOA_Ptr": "0x0003EAB2",
			"FOA_Rva": "0x00040E5C"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003EACA",
			"FOA_Rva": "0x00040EC3"
		},
		{
			"Text_Raw": "「優里が守ってくれるなら、なんの心配もないね」",
			"Text_Tra": "「有優里守護著的話，我就不用擔心了。」",
			"FOA_Ptr": "0x0003EB0A",
			"FOA_Rva": "0x00040ECA"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003EB22",
			"FOA_Rva": "0x00040EFA"
		},
		{
			"Text_Raw": "寄り添っていてくれるという言葉を彼女は果たすつもりなのだ。",
			"Text_Tra": "她打算要實現相互依偎的諾言。",
			"FOA_Ptr": "0x0003EB62",
			"FOA_Rva": "0x00040F01"
		},
		{
			"Text_Raw": "それならもう、背後を振り向く必要もない。",
			"Text_Tra": "這樣的話，就沒有必要再回頭了。",
			"FOA_Ptr": "0x0003EBBA",
			"FOA_Rva": "0x00040F3E"
		},
		{
			"Text_Raw": "「まあ、護衛隊ってことで普段は背中を守りつつ、八剣が暴走したら取り締まるのも仕事の内だろうけどね」",
			"Text_Tra": "「不過，護衛隊除了平常要守住後方外，當有八劍失控時，將其約束住也是工作之一哦。」",
			"FOA_Ptr": "0x0003EC12",
			"FOA_Rva": "0x00040F69"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003EC2A",
			"FOA_Rva": "0x00040FCD"
		},
		{
			"Text_Raw": "その力や技術は、優里に学んでもらった。",
			"Text_Tra": "這力量與技術，優里已經學到了。",
			"FOA_Ptr": "0x0003EC6A",
			"FOA_Rva": "0x00040FD4"
		},
		{
			"Text_Raw": "僕の補足に、優里がずっと浮かべていた笑顔をひっこめる。",
			"Text_Tra": "在我做補充時，優里始終帶著微笑。",
			"FOA_Ptr": "0x0003ECC2",
			"FOA_Rva": "0x00040FFD"
		},
		{
			"Text_Raw": "「もう。余計な一言を……どうしてあなたは、気分よくすっきり同意してくれないのでしょうね」",
			"Text_Tra": "「真是的。這句話是多餘的……反正到你這個人也不會乖乖就範的。」",
			"FOA_Ptr": "0x0003ED1A",
			"FOA_Rva": "0x00041036"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003ED32",
			"FOA_Rva": "0x0004109F"
		},
		{
			"Text_Raw": "「あはは。それが僕だからね」",
			"Text_Tra": "「啊哈哈。我就是這樣嘛。」",
			"FOA_Ptr": "0x0003ED72",
			"FOA_Rva": "0x000410A6"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003ED8A",
			"FOA_Rva": "0x000410C4"
		},
		{
			"Text_Raw": "冗談のように笑うが――実際は真剣だった。",
			"Text_Tra": "雖然我像是玩笑般哈哈笑著――但其實是認真的。",
			"FOA_Ptr": "0x0003EDCA",
			"FOA_Rva": "0x000410CB"
		},
		{
			"Text_Raw": "僕や春姫だって間違いは犯すだろう。",
			"Text_Tra": "我跟春姬的話肯定是會有這問題的。",
			"FOA_Ptr": "0x0003EE22",
			"FOA_Rva": "0x000410F6"
		},
		{
			"Text_Raw": "その時、八剣であろうとも間違いを正してくれる存在があればこそ、僕らの肩の荷が少しは軽くなるのだ。",
			"Text_Tra": "到時，正因為有人連對八劍所犯的錯誤都能加以導正，我們的負擔才得以減輕。",
			"FOA_Ptr": "0x0003EE7A",
			"FOA_Rva": "0x0004111B"
		},
		{
			"Text_Raw": "「とにかく、明日からは喫茶店をやりつつ、護衛隊の設立までは僕も手伝うよ」",
			"Text_Tra": "「總之，從明天開始就繼續做喫茶店，護衛隊的創設那邊我也會幫忙的。」",
			"FOA_Ptr": "0x0003EED2",
			"FOA_Rva": "0x0004117E"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003EEEA",
			"FOA_Rva": "0x000411C8"
		},
		{
			"Text_Raw": "「それからは、あなたもご自身の夢を見つけるのですね」",
			"Text_Tra": "「之後，你也要去尋找自己的夢想了吧。」",
			"FOA_Ptr": "0x0003EF2A",
			"FOA_Rva": "0x000411CF"
		},
		{
			"Text_Raw": "優　里",
			"Text_Tra": "優里",
			"FOA_Ptr": "0x0003EF42",
			"FOA_Rva": "0x00041214"
		},
		{
			"Text_Raw": "「うん」",
			"Text_Tra": "「恩。」",
			"FOA_Ptr": "0x0003EF82",
			"FOA_Rva": "0x0004121B"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003EF9A",
			"FOA_Rva": "0x00041225"
		},
		{
			"Text_Raw": "笑顔で想いをはせる。",
			"Text_Tra": "面帶笑容地想著。",
			"FOA_Ptr": "0x0003EFDA",
			"FOA_Rva": "0x0004122C"
		},
		{
			"Text_Raw": "僕は何者になるのだろう？",
			"Text_Tra": "我要成為什麼呢？",
			"FOA_Ptr": "0x0003F032",
			"FOA_Rva": "0x00041243"
		},
		{
			"Text_Raw": "新しい一歩は、本当のスタートラインを見つけることから始めよう。",
			"Text_Tra": "新的一步，就從找出真正的起跑點開始吧。",
			"FOA_Ptr": "0x0003F08A",
			"FOA_Rva": "0x0004125E"
		}
	]
}