		},
		{
			"Text_Raw": "。、」』）！？”～ー♪",
			"Text_Tra": "。、」』）！？”～ー～",
			"FOA_Ptr": "0x00003408",
			"FOA_Rva": "0x0003ECBA"
		},
		{
			"Text_Raw": "…―─",
			"Text_Tra": "…―─",
			"FOA_Ptr": "0x00019C42",
			"FOA_Rva": "0x0003FAC0"
		},
		{
			"Text_Raw": "。、」』）！？”～ー♪",
			"Text_Tra": "。、」』）！？”～ー～",
			"FOA_Ptr": "0x00019C4E",
			"FOA_Rva": "0x0003FAC7"
		},
		{
			"Text_Raw": "…―─",
			"Text_Tra": "…―─",
			"FOA_Ptr": "0x0001AC98",
			"FOA_Rva": "0x0003FBBF"
		},
		{
			"Text_Raw": "。、」』）！？”～ー♪",
			"Text_Tra": "。、」』）！？”～ー～",
			"FOA_Ptr": "0x0001ACA4",
			"FOA_Rva": "0x0003FBC6"
		},
		{
			"Text_Raw": "- 双子本筋０６－０２ -",
			"Text_Tra": "- 双胞胎主线06-02 -",
			"FOA_Ptr": "0x0003D7DC",
			"FOA_Rva": "0x0004007F"
		},
		{
			"Text_Raw": "「２人とも手を繋いで、さらに未来へ進む」",
			"Text_Tra": "「我想牵着你们俩个的手，一起往未来前进。」",
			"FOA_Ptr": "0x0003D80A",
			"FOA_Rva": "0x00040096"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D822",
			"FOA_Rva": "0x000400C0"
		},
		{
			"Text_Raw": "笑顔で答える。",
			"Text_Tra": "我笑着回答。",
			"FOA_Ptr": "0x0003D862",
			"FOA_Rva": "0x000400C7"
		},
		{
			"Text_Raw": "「別に誤魔化してるわけじゃないよ」",
			"Text_Tra": "「这不是在敷衍哦。」",
			"FOA_Ptr": "0x0003D8BA",
			"FOA_Rva": "0x000400D8"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D8D2",
			"FOA_Rva": "0x000400FC"
		},
		{
			"Text_Raw": "「茜ちゃんが人を好きなるって気持ちがはっきりしてからのほうが、確かに公平だろうし――」",
			"Text_Tra": "「在小茜清楚了解到喜欢一个人是什么感觉后，才比较公平――」",
			"FOA_Ptr": "0x0003D912",
			"FOA_Rva": "0x00040103"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D92A",
			"FOA_Rva": "0x0004015B"
		},
		{
			"Text_Raw": "「僕のほうは、そもそも、茜ちゃんと葵ちゃんを好きすぎるくらいだから、いつでもＯＫ」",
			"Text_Tra": "「而且我啊，本来就太喜欢太喜欢小茜跟小葵了，所以随时都是OK的哦。」",
			"FOA_Ptr": "0x0003D96A",
			"FOA_Rva": "0x00040162"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003D982",
			"FOA_Rva": "0x000401B6"
		},
		{
			"Text_Raw": "「あはは♪」",
			"Text_Tra": "「啊哈哈～」",
			"FOA_Ptr": "0x0003DACE",
			"FOA_Rva": "0x000401BD"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003DAE6",
			"FOA_Rva": "0x000401DA"
		},
		{
			"Text_Raw": "葵が楽しそうに認めてくれる。",
			"Text_Tra": "葵开心地同意了。",
			"FOA_Ptr": "0x0003DB26",
			"FOA_Rva": "0x000401E1"
		},
		{
			"Text_Raw": "彼女はパチンと手を打ち、花をかたどっていた雷球が美しく弾けた。",
			"Text_Tra": "她啪的一下把手阖起，花变成了漂亮的雷球飞散了开。",
			"FOA_Ptr": "0x0003DB7E",
			"FOA_Rva": "0x00040200"
		},
		{
			"Text_Raw": "「うん。わかった。話してすっきりした。信お兄ちゃんありがとう♪」",
			"Text_Tra": "「嗯。我知道了。说完后舒服多了。谢谢信哥哥～」",
			"FOA_Ptr": "0x0003DC36",
			"FOA_Rva": "0x00040241"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003DC4E",
			"FOA_Rva": "0x00040292"
		},
		{
			"Text_Raw": "「いつでもどうぞ」",
			"Text_Tra": "「随时欢迎。」",
			"FOA_Ptr": "0x0003DC8E",
			"FOA_Rva": "0x00040299"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DCA6",
			"FOA_Rva": "0x000402AD"
		},
		{
			"Text_Raw": "「いつでも？」",
			"Text_Tra": "「随时吗？」",
			"FOA_Ptr": "0x0003DCE6",
			"FOA_Rva": "0x000402B4"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003DCFE",
			"FOA_Rva": "0x000402D3"
		},
		{
			"Text_Raw": "「寝込みを襲う以外で」",
			"Text_Tra": "「除了趁我睡着时偷袭之外。」",
			"FOA_Ptr": "0x0003DD3E",
			"FOA_Rva": "0x000402DA"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DD56",
			"FOA_Rva": "0x000402F2"
		},
		{
			"Text_Raw": "先回りして答えておく。",
			"Text_Tra": "这个先说在前面。",
			"FOA_Ptr": "0x0003DD96",
			"FOA_Rva": "0x000402F9"
		},
		{
			"Text_Raw": "「なーんだ。残念」",
			"Text_Tra": "「啊～。真可惜。」",
			"FOA_Ptr": "0x0003DDEE",
			"FOA_Rva": "0x00040312"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003DE06",
			"FOA_Rva": "0x00040335"
		},
		{
			"Text_Raw": "葵がてへ、と笑う。",
			"Text_Tra": "葵噗哧一笑。",
			"FOA_Ptr": "0x0003DE46",
			"FOA_Rva": "0x0004033C"
		},
		{
			"Text_Raw": "「じゃあ、葵は帰るね。お姉ちゃんのお腹に布団をかけてあげないと」",
			"Text_Tra": "「那，葵就回去啰。得要去帮姊姊的肚子盖上被子呢。」",
			"FOA_Ptr": "0x0003DE9E",
			"FOA_Rva": "0x00040351"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003DEB6",
			"FOA_Rva": "0x000403A2"
		},
		{
			"Text_Raw": "「また遊ぼうね」",
			"Text_Tra": "「之后再来玩吧。」",
			"FOA_Ptr": "0x0003DEF6",
			"FOA_Rva": "0x000403A9"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003DF0E",
			"FOA_Rva": "0x000403BB"
		},
		{
			"Text_Raw": "手を振って見送ろうとする。",
			"Text_Tra": "我挥着手准备目送她。",
			"FOA_Ptr": "0x0003DF4E",
			"FOA_Rva": "0x000403C2"
		},
		{
			"Text_Raw": "と。",
			"Text_Tra": "突然。",
			"FOA_Ptr": "0x0003DFA6",
			"FOA_Rva": "0x000403DF"
		},
		{
			"Text_Raw": "「えい」",
			"Text_Tra": "「嘿。」",
			"FOA_Ptr": "0x0003E066",
			"FOA_Rva": "0x000403E6"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003E07E",
			"FOA_Rva": "0x000403FF"
		},
		{
			"Text_Raw": "のんきな声で、葵が拳を突きだす。",
			"Text_Tra": "随着温吞的一声，葵挥过来一拳。",
			"FOA_Ptr": "0x0003E0CE",
			"FOA_Rva": "0x00040406"
		},
		{
			"Text_Raw": "僕の腹に向けて。",
			"Text_Tra": "朝着我的腹部。",
			"FOA_Ptr": "0x0003E126",
			"FOA_Rva": "0x00040429"
		},
		{
			"Text_Raw": "声は軽かったが、至近距離から、今までで最速、電光石火の一撃だった。",
			"Text_Tra": "声音虽然温吞，但是这样的超近距离，并且是以目前最快速度所挥出的电光石火一击。",
			"FOA_Ptr": "0x0003E17E",
			"FOA_Rva": "0x0004043C"
		},
		{
			"Text_Raw": "その手首を軽く掴む。",
			"Text_Tra": "我轻轻地抓住她的手腕。",
			"FOA_Ptr": "0x0003E1D6",
			"FOA_Rva": "0x00040481"
		},
		{
			"Text_Raw": "「……また強くなったね」",
			"Text_Tra": "「……又变强了呢。」",
			"FOA_Ptr": "0x0003E22E",
			"FOA_Rva": "0x00040498"
		},
		{
			"Text_Raw": "　葵　",
			"Text_Tra": "葵",
			"FOA_Ptr": "0x0003E246",
			"FOA_Rva": "0x000404C1"
		},
		{
			"Text_Raw": "葵が感心したように言う。",
			"Text_Tra": "葵佩服地说道。",
			"FOA_Ptr": "0x0003E286",
			"FOA_Rva": "0x000404C8"
		},
		{
			"Text_Raw": "それを確かめ、今度こそ葵はなにも言わずに去って行った。",
			"Text_Tra": "确认完后，葵就没再多说什么，只是静静地离开了。",
			"FOA_Ptr": "0x0003E336",
			"FOA_Rva": "0x000404E3"
		},
		{
			"Text_Raw": "「…………」",
			"Text_Tra": "「…………」",
			"FOA_Ptr": "0x0003E38E",
			"FOA_Rva": "0x0004051C"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E3A6",
			"FOA_Rva": "0x0004052A"
		},
		{
			"Text_Raw": "僕は、葵の手首をつかんだ、自分の手のひらを見つめる。",
			"Text_Tra": "我摊开刚刚抓住葵手腕的那只手，看着手掌心。",
			"FOA_Ptr": "0x0003E3E6",
			"FOA_Rva": "0x00040531"
		},
		{
			"Text_Raw": "また、強くなった。",
			"Text_Tra": "又变强了呢。",
			"FOA_Ptr": "0x0003E43E",
			"FOA_Rva": "0x00040568"
		},
		{
			"Text_Raw": "仕方なく強くなってしまった。",
			"Text_Tra": "又是迫于无奈的变强了呢。",
			"FOA_Ptr": "0x0003E496",
			"FOA_Rva": "0x0004057D"
		},
		{
			"Text_Raw": "戦いは終わったが――まだ答えの出ていない疑問がある。",
			"Text_Tra": "战斗虽然结束了――但还有没能解答的问题。",
			"FOA_Ptr": "0x0003E4EE",
			"FOA_Rva": "0x0004059C"
		},
		{
			"Text_Raw": "あの死鬼の襲撃がなにを意味していたのか。",
			"Text_Tra": "像是这次死鬼的袭击到底是意味着什么。",
			"FOA_Ptr": "0x0003E546",
			"FOA_Rva": "0x000405D3"
		},
		{
			"Text_Raw": "ただでさえ稀有な死鬼が２体同時に、カントの中心である職人街を、八剣の戦闘員が揃っているタイミングで襲撃した……。",
			"Text_Tra": "如此稀有的死鬼竟然两只一起来，而且还在康托中心的工匠街，并且又在八剑齐聚的时候发动袭击……",
			"FOA_Ptr": "0x0003E59E",
			"FOA_Rva": "0x000405FE"
		},
		{
			"Text_Raw": "「……誰がやったんだ？」",
			"Text_Tra": "「……是谁做的？」",
			"FOA_Ptr": "0x0003E5F6",
			"FOA_Rva": "0x00040671"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E60E",
			"FOA_Rva": "0x0004068B"
		},
		{
			"Text_Raw": "検討するまでもなく、あの戦いは偶然ではない。",
			"Text_Tra": "想都不用想，就知道那场战斗绝非偶然。",
			"FOA_Ptr": "0x0003E64E",
			"FOA_Rva": "0x00040692"
		},
		{
			"Text_Raw": "誰かか――なにかが、目的をもって戦場を用意した。",
			"Text_Tra": "是某人――或是什么东西，带有目的性地准备好了战场。",
			"FOA_Ptr": "0x0003E6A6",
			"FOA_Rva": "0x000406C1"
		},
		{
			"Text_Raw": "目的＝僕の成長以外、なにがある。",
			"Text_Tra": "目的＝除了我的成长外，还有其他的。",
			"FOA_Ptr": "0x0003E6FE",
			"FOA_Rva": "0x000406F4"
		},
		{
			"Text_Raw": "「盤上の駒になるつもりはない」",
			"Text_Tra": "「我可不想当棋盘上的棋子。」",
			"FOA_Ptr": "0x0003E78A",
			"FOA_Rva": "0x00040717"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E7A2",
			"FOA_Rva": "0x00040737"
		},
		{
			"Text_Raw": "拳を握りしめる。",
			"Text_Tra": "握起了拳头。",
			"FOA_Ptr": "0x0003E7E2",
			"FOA_Rva": "0x0004073E"
		},
		{
			"Text_Raw": "人類の命運を賭けた戦いは、終わった。",
			"Text_Tra": "赌上人类命运的战斗，已经结束了。",
			"FOA_Ptr": "0x0003E83A",
			"FOA_Rva": "0x00040751"
		},
		{
			"Text_Raw": "千年前からの知識の継承も済んだ。",
			"Text_Tra": "千年前的知识也都传承完毕了。",
			"FOA_Ptr": "0x0003E892",
			"FOA_Rva": "0x00040778"
		},
		{
			"Text_Raw": "「ここからが、僕の本当の戦いの始まりだ」",
			"Text_Tra": "「接下来，就是我真正战斗的开始。」",
			"FOA_Ptr": "0x0003E8EA",
			"FOA_Rva": "0x0004079B"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003E902",
			"FOA_Rva": "0x000407C5"
		},
		{
			"Text_Raw": "宣言する。",
			"Text_Tra": "我如此宣言道。",
			"FOA_Ptr": "0x0003E942",
			"FOA_Rva": "0x000407CC"
		},
		{
			"Text_Raw": "そして、僕は座り込む。",
			"Text_Tra": "然后，坐了下来。",
			"FOA_Ptr": "0x0003EA06",
			"FOA_Rva": "0x000407D9"
		},
		{
			"Text_Raw": "「……もうちょっと猫を撫でてから帰ろう」",
			"Text_Tra": "「……回去前再多摸一下猫吧」",
			"FOA_Ptr": "0x0003EA5E",
			"FOA_Rva": "0x000407F2"
		},
		{
			"Text_Raw": "　信　",
			"Text_Tra": "信",
			"FOA_Ptr": "0x0003EA76",
			"FOA_Rva": "0x0004081C"
		},
		{
			"Text_Raw": "カントの神様は可愛いのだ。",
			"Text_Tra": "康托的神明真是可爱呢。",
			"FOA_Ptr": "0x0003EAB6",
			"FOA_Rva": "0x00040823"
		},
		{
			"Text_Raw": "- セカンドオープニング -",
			"Text_Tra": "- 第二开场 -",
			"FOA_Ptr": "0x0003EB8A",
			"FOA_Rva": "0x00040840"
		}
	]
}